{
  "name": "account",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run migrate:up",
    "build": "rm -rf dist && tsc",
    "start": "node dist/src/index.js",
    "dev": "nodemon --exec ts-node ./src/index.ts --mode development",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "pretest": "cross-env TESTIFY_NODE_ENV=test npm run seed:reset:test && npm run migrate-test:reset && npm run migrate-test:up && npm run seed:up:test",
    "test": "cross-env TESTIFY_NODE_ENV=test nyc --reporter=lcov --reporter=text --reporter=text-summary mocha --require ts-node/register --exit --recursive --timeout=150000 tests/index.ts",
    "migrations": "source .env && db-migrate",
    "create:migration": "db-migrate create --config database.json",
    "migrate:up": "db-migrate up  --config database.json",
    "migrate:down": "db-migrate down  --config database.json",
    "migrate-test:up": "db-migrate up --config database.json -e test",
    "migrate-test:down": "db-migrate down -c 100 --config database.json -e test",
    "migrate:reset": "db-migrate reset --config database.json",
    "migrate-test:reset": "db-migrate reset --config database.json -e test",
    "seed:create": "db-migrate create:seed --config database.json",
    "seed:up": "db-migrate up:seed --config database.json",
    "seed:down": "db-migrate down:seed --config database.json",
    "seed:reset": "db-migrate reset:seed --config database.json",
    "seed:reset:test": "db-migrate reset:seed --config database.json -e test",
    "seed:up:test": "db-migrate up:seed --config database.json -e test"
  },
  "dependencies": {
    "@sentry/node": "^10.25.0",
    "aws-sdk": "^2.1424.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.0",
    "bluebird": "^3.7.2",
    "chai-http": "^4.4.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.3.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "firebase-admin": "^13.4.0",
    "generate-password": "^1.7.1",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "joi-password": "^4.1.1",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "mocha": "^10.2.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-mailjet": "^6.0.6",
    "nodemailer": "^6.10.0",
    "pg-promise": "^11.5.4",
    "sinon": "^15.2.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "winston-loki": "^6.1.3",
    "winston-papertrail": "^1.0.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bluebird": "^3.5.42",
    "@types/chai": "^4.3.5",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.17.0",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/sinon": "^10.0.16",
    "@types/uuid": "^9.0.2",
    "chai": "^4.3.7",
    "eslint": "^8.43.0",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "*"
  }
}
